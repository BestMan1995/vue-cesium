{
  "name": "vue-cesium",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "3.0.0-beta",
  "description": "Vue 3.x components for CesiumJS.",
  "scripts": {
    "cz": "npx git-cz",
    "test": "jest --coverage && coveralls < coverage/lcov.info",
    "gen": "bash ./scripts/gc.sh",
    "gen:version": "esno build/gen-version.js",
    "update:version": "esno build/update-version.ts",
    "bootstrap": "yarn --frozen-lockfile && npx lerna bootstrap && yarn gen:version",
    "clean:lib": "rimraf lib && rimraf es && rimraf dist",
    "build": "sh scripts/build.sh",
    "build:helper": "esno build/build-helper.ts",
    "build:helper-zh": "node build/build-helper-zh.js",
    "build:indices": "esno build/build-indices.ts",
    "build:comps": "rimraf dist/components && esno build/components.ts",
    "build:style": "gulp --cwd ./build",
    "build:prod": "sh scripts/publish.sh",
    "build:directives": "cd packages/directives && yarn clean && yarn build",
    "build:composables": "cd packages/composables && yarn clean && yarn build",
    "build:locale": "cd packages/locale && yarn clean && yarn build",
    "build:shared": "cd packages/shared && yarn clean && yarn build",
    "build:theme": "cd packages/theme-default && yarn clean && yarn build",
    "build:utils": "cd packages/utils && yarn clean && yarn build",
    "build:full-bundle": "esno build/full-bundle.ts",
    "format": "prettier --write .",
    "lint": "eslint ./packages --ext .vue,.js,.ts,.jsx,.tsx && prettier --check .",
    "lint:fix": "eslint --fix ./packages --ext .vue,.js,.ts,.jsx,.tsx && prettier --write .",
    "website-build": "rimraf website-dist && cross-env NODE_ENV=production webpack --config ./website/webpack.config.js",
    "website-dev": "webpack-dev-server --config ./website/webpack.config.js",
    "website-dev:play": "cross-env PLAY_ENV=true yarn website-dev",
    "upgrade:vue": "bash ./scripts/update-versions.sh",
    "prepare": "husky install"
  },
  "author": "zouyaoji <370681295@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zouyaoji/vue-cesium.git"
  },
  "homepage": "https://zouyaoji.top/vue-cesium",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,css,json,yaml,yml,md,html}": "prettier --write"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "vue": "^3.2.11"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "heatmap.js": "^2.0.5",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "proj4": "^2.7.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/cz-commitlint": "^13.1.0",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/gulp": "^4.0.9",
    "@types/heatmap.js": "^2.0.36",
    "@types/jest": "^26.0.20",
    "@types/klaw-sync": "^6.0.1",
    "@types/lodash": "^4.14.168",
    "@types/merge-descriptors": "^1.0.1",
    "@types/proj4": "^2.5.0",
    "@types/webpack-dev-server": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.14.2",
    "@vue/babel-plugin-jsx": "1.0.7",
    "@vue/compiler-sfc": "3.2.8",
    "@vue/component-compiler-utils": "^3.2.2",
    "algoliasearch": "^4.10.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-vue": "^2.0.2",
    "chalk": "^4.1.2",
    "clipboard-copy": "^4.0.1",
    "components-helper": "^1.0.2",
    "copy-webpack-plugin": "6.4.1",
    "coveralls": "^3.1.0",
    "cp-cli": "^2.0.0",
