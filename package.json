    "throttle-debounce": "2.3.0",
    "through2": "^4.0.2",
    "transliteration": "^2.2.0",
    "ts-loader": "^8.0.15",
    "ts-morph": "^11.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "vue": "^3.2.11",
    "vue-jest": "5.0.0-alpha.5",
    "vue-loader": "^16.1.2",
    "vue-router": "^4.0.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
